
{
  "manifest_version": 3,
  "name": "GitHub Repo Dates",
  "version": "1.0.0",
  "description": "Shows repository creation date, first commit date, and latest commit date in GitHub UI",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://github.com/*",
    "https://api.github.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}